/* Add subtle animation to the background */
@keyframes float-icons {
  0%, 100% { transform: translateY(0px) translateX(0px); }
  25% { transform: translateY(-2px) translateX(1px); }
  50% { transform: translateY(0px) translateX(-1px); }
  75% { transform: translateY(1px) translateX(0px); }
}

.blog-post-container::before,
.blog-listing-container::before {
  animation: float-icons 60s ease-in-out infinite;
}