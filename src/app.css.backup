@import "tailwindcss";

@import url('https://fonts.googleapis.com/css2?family=Inter:wght@300;400;500;600;700&family=JetBrains+Mono:wght@400;500;600&display=swap');

@theme {
  --color-primary-50: #f0f9ff;
  --color-primary-100: #e0f2fe;
  --color-primary-200: #bae6fd;
  --color-primary-300: #7dd3fc;
  --color-primary-400: #38bdf8;
  --color-primary-500: #0ea5e9;
  --color-primary-600: #0284c7;
  --color-primary-700: #0369a1;
  --color-primary-800: #075985;
  --color-primary-900: #0c4a6e;
  
  --color-accent-50: #f0fdf4;
  --color-accent-100: #dcfce7;
  --color-accent-200: #bbf7d0;
  --color-accent-300: #86efac;
  --color-accent-400: #4ade80;
  --color-accent-500: #22c55e;
  --color-accent-600: #16924f;
  --color-accent-700: #15803d;
  --color-accent-800: #166534;
  --color-accent-900: #14532d;
  
  --font-family-sans: 'Inter', system-ui, sans-serif;
  --font-family-mono: 'JetBrains Mono', monospace;
  
  --animate-float: float 6s ease-in-out infinite;
  --animate-glow: glow 2s ease-in-out infinite alternate;
  --animate-slide-up: slideUp 0.5s ease-out;
  --animate-fade-in: fadeIn 0.6s ease-out;
  --animate-snap-in: snapIn 0.4s cubic-bezier(0.25, 0.46, 0.45, 0.94);
  --animate-snap-out: snapOut 0.3s cubic-bezier(0.55, 0.055, 0.675, 0.19);
  --animate-slide-in-left: slideInFromLeft 0.3s ease-out;
  --animate-pulse: pulse 2s ease-in-out infinite;
  
  --default-transition-duration: 300ms;
}

/* Dark mode configuration */
@media (prefers-color-scheme: dark) {
  :root {
    color-scheme: dark;
  }
}

.dark {
  color-scheme: dark;
}

@keyframes pulseBorder {
  0%, 100% { 
    box-shadow: 
      0 0 0 0 rgba(14, 165, 233, 0.4),
      0 0 0 0 rgba(34, 197, 94, 0.3);
  }
  50% { 
    box-shadow: 
      0 0 0 4px rgba(14, 165, 233, 0.2),
      0 0 0 8px rgba(34, 197, 94, 0.1);
  }
}

@keyframes snapIn {
  0% { 
    transform: translateY(20px) scale(0.95); 
    opacity: 0.8; 
  }
  100% { 
    transform: translateY(0) scale(1); 
    opacity: 1; 
  }
}

@keyframes snapOut {
  0% { 
    transform: translateY(0) scale(1); 
    opacity: 1; 
  }
  100% { 
    transform: translateY(-20px) scale(0.95); 
    opacity: 0.8; 
  }
}

@keyframes slideInFromLeft {
  0% { 
    transform: translateX(-100%); 
    opacity: 0; 
  }
  100% { 
    transform: translateX(0); 
    opacity: 1; 
  }
}

@keyframes float {
  0%, 100% { 
    transform: translateY(0px) rotate(0deg); 
  }
  50% { 
    transform: translateY(-20px) rotate(180deg); 
  }
}

@keyframes glow {
  0%, 100% { 
    box-shadow: 0 0 5px rgba(14, 165, 233, 0.5); 
  }
  50% { 
    box-shadow: 0 0 20px rgba(14, 165, 233, 0.8), 0 0 30px rgba(34, 197, 94, 0.4); 
  }
}

@keyframes slideUp {
  from { 
    opacity: 0; 
    transform: translateY(30px); 
  }
  to { 
    opacity: 1; 
    transform: translateY(0); 
  }
}

@keyframes fadeIn {
  from { 
    opacity: 0; 
    transform: scale(0.95); 
  }
  to { 
    opacity: 1; 
    transform: scale(1); 
  }
}

@keyframes pulse {
  0%, 100% { 
    transform: scale(1); 
    opacity: 1; 
  }
  50% { 
    transform: scale(1.05); 
    opacity: 0.8; 
  }
}

/* Advanced Morphing Animations */
@keyframes morphBounce {
  0% { 
    transform: scale(1) rotate(0deg); 
  }
  25% { 
    transform: scale(1.1) rotate(5deg); 
  }
  50% { 
    transform: scale(0.95) rotate(-3deg); 
  }
  75% { 
    transform: scale(1.05) rotate(2deg); 
  }
  100% { 
    transform: scale(1) rotate(0deg); 
  }
}

@keyframes liquidMorph {
  0%, 100% { 
    border-radius: 12px; 
    transform: scale(1); 
  }
  25% { 
    border-radius: 50% 20% 80% 30%; 
    transform: scale(1.02); 
  }
  50% { 
    border-radius: 20% 80% 30% 50%; 
    transform: scale(0.98); 
  }
  75% { 
    border-radius: 80% 30% 50% 20%; 
    transform: scale(1.01); 
  }
}

@keyframes glowPulse {
  0%, 100% { 
    box-shadow: 
      0 0 5px rgba(14, 165, 233, 0.3),
      0 0 10px rgba(14, 165, 233, 0.2),
      0 0 15px rgba(14, 165, 233, 0.1);
  }
  50% { 
    box-shadow: 
      0 0 10px rgba(14, 165, 233, 0.6),
      0 0 20px rgba(14, 165, 233, 0.4),
      0 0 30px rgba(14, 165, 233, 0.2),
      0 0 40px rgba(34, 197, 94, 0.1);
  }
}

@keyframes textShimmer {
  0% { 
    background-position: -200% center; 
  }
  100% { 
    background-position: 200% center; 
  }
}

@keyframes particleFloat {
  0%, 100% { 
    transform: translateY(0px) translateX(0px) rotate(0deg); 
    opacity: 0.3; 
  }
  25% { 
    transform: translateY(-10px) translateX(5px) rotate(90deg); 
    opacity: 0.7; 
  }
  50% { 
    transform: translateY(-20px) translateX(-5px) rotate(180deg); 
    opacity: 1; 
  }
  75% { 
    transform: translateY(-10px) translateX(-10px) rotate(270deg); 
    opacity: 0.7; 
  }
}

@keyframes magneticPull {
  0% { 
    transform: scale(1) rotate(0deg); 
  }
  50% { 
    transform: scale(1.05) rotate(2deg); 
  }
  100% { 
    transform: scale(1) rotate(0deg); 
  }
}

/* Enhanced Hover Effects */
.hover-lift {
  transition: all 0.3s cubic-bezier(0.25, 0.46, 0.45, 0.94);
}

.hover-lift:hover {
  transform: translateY(-8px) scale(1.02);
  box-shadow: 
    0 20px 40px rgba(0, 0, 0, 0.1),
    0 0 0 1px rgba(255, 255, 255, 0.1);
}

.hover-glow:hover {
  animation: glowPulse 2s ease-in-out infinite;
}

.hover-morph:hover {
  animation: liquidMorph 2s ease-in-out infinite;
}

.hover-bounce:hover {
  animation: morphBounce 0.6s ease-in-out;
}

/* Text Effects */
.gradient-text {
  background: linear-gradient(135deg, #0ea5e9, #22c55e, #8b5cf6);
  background-size: 200% 200%;
  -webkit-background-clip: text;
  background-clip: text;
  -webkit-text-fill-color: transparent;
  animation: textShimmer 3s ease-in-out infinite;
}

.text-glow {
  text-shadow: 
    0 0 10px rgba(14, 165, 233, 0.5),
    0 0 20px rgba(14, 165, 233, 0.3),
    0 0 30px rgba(14, 165, 233, 0.1);
}

/* Glass Effects */
.glass-effect {
  background: rgba(255, 255, 255, 0.1);
  backdrop-filter: blur(20px);
  border: 1px solid rgba(255, 255, 255, 0.2);
  box-shadow: 
    0 8px 32px rgba(0, 0, 0, 0.1),
    inset 0 1px 0 rgba(255, 255, 255, 0.2);
}

.glass-effect:hover {
  background: rgba(255, 255, 255, 0.15);
  border-color: rgba(255, 255, 255, 0.3);
}

/* Neon Effects */
.neon-border {
  border: 2px solid transparent;
  background: linear-gradient(45deg, #0ea5e9, #22c55e) border-box;
  border-radius: 12px;
  position: relative;
}

.neon-border::before {
  content: '';
  position: absolute;
  inset: 0;
  padding: 2px;
  background: linear-gradient(45deg, #0ea5e9, #22c55e, #8b5cf6);
  border-radius: inherit;
  mask: linear-gradient(#fff 0 0) content-box, linear-gradient(#fff 0 0);
  mask-composite: exclude;
  animation: glowPulse 2s ease-in-out infinite;
}

/* Particle System Enhancements */
.enhanced-particle {
  animation: particleFloat 8s ease-in-out infinite;
  filter: blur(0.5px);
}

.enhanced-particle:nth-child(odd) {
  animation-delay: -2s;
  animation-duration: 12s;
}

.enhanced-particle:nth-child(even) {
  animation-delay: -4s;
  animation-duration: 10s;
}

/* Magnetic Elements */
.magnetic {
  transition: transform 0.2s cubic-bezier(0.25, 0.46, 0.45, 0.94);
}

/* Performance Optimizations */
.gpu-accelerated {
  transform: translateZ(0);
  will-change: transform, opacity;
}

/* Accessibility Enhancements */
@media (prefers-reduced-motion: reduce) {
  * {
    animation-duration: 0.01ms !important;
    animation-iteration-count: 1 !important;
    transition-duration: 0.01ms !important;
  }
}

/* Dark Mode Enhancements */
.dark .glass-effect {
  background: rgba(0, 0, 0, 0.2);
  border-color: rgba(255, 255, 255, 0.1);
}

.dark .neon-border {
  box-shadow: 
    0 0 20px rgba(14, 165, 233, 0.3),
    inset 0 1px 0 rgba(255, 255, 255, 0.1);
}

@keyframes glow {
  0% { box-shadow: 0 0 5px #0ea5e9, 0 0 10px #0ea5e9, 0 0 15px #0ea5e9; }
  100% { box-shadow: 0 0 10px #0ea5e9, 0 0 20px #0ea5e9, 0 0 30px #0ea5e9; }
}

@keyframes slideUp {
  0% { transform: translateY(100px); opacity: 0; }
  100% { transform: translateY(0); opacity: 1; }
}

@keyframes fadeIn {
  0% { opacity: 0; }
  100% { opacity: 1; }
}

@layer base {
  html {
    scroll-behavior: smooth;
  }
  
  body {
    @apply font-sans antialiased transition-colors duration-300;
  }
  
  /* Sections styling */
  section {
    padding-top: 80px; /* Account for fixed navigation */
  }
  
  /* Ensure large content sections work properly */
  section > * {
    max-width: 100%;
  }
  
  /* Hero section specific adjustments */
  section#home {
    padding-top: 100px; /* Extra top padding for mobile */
    padding-bottom: 40px;
  }
  
  @media (min-width: 768px) {
    section#home {
      padding-top: 80px;
      padding-bottom: 20px;
    }
  }
  
  /* Light mode styles */
  :root {
    --bg-primary: white;
    --bg-secondary: rgb(249, 250, 251);
    --text-primary: rgb(17, 24, 39);
    --text-secondary: rgb(55, 65, 81);
    --text-muted: rgb(75, 85, 99);
  }
  
  /* Dark mode styles */
  .dark {
    --bg-primary: rgb(15, 23, 42);
    --bg-secondary: rgb(30, 41, 59);
    --text-primary: rgb(255, 255, 255);
    --text-secondary: rgb(226, 232, 240);
    --text-muted: rgb(148, 163, 184);
  }
  
  body {
    background-color: var(--bg-primary);
    color: var(--text-primary);
  }
}

@layer components {
  .section-padding {
    @apply px-4 sm:px-6 lg:px-8;
  }
  
  .container-max {
    @apply max-w-7xl mx-auto;
  }
  
  .gradient-text {
    @apply bg-gradient-to-r from-primary-500 to-accent-600 bg-clip-text text-transparent;
  }
  
  /* Ensure gradient text stays gradient in dark mode */
  .dark .gradient-text {
    background: linear-gradient(to right, rgb(14, 165, 233), rgb(22, 146, 79)) !important;
    -webkit-background-clip: text !important;
    background-clip: text !important;
    color: transparent !important;
  }
  
  /* Extra specificity for gradient text headings in dark mode */
  .dark h1.gradient-text,
  .dark h2.gradient-text,
  .dark h3.gradient-text,
  .dark h4.gradient-text,
  .dark h5.gradient-text,
  .dark h6.gradient-text {
    background: linear-gradient(to right, rgb(14, 165, 233), rgb(22, 146, 79)) !important;
    -webkit-background-clip: text !important;
    background-clip: text !important;
    color: transparent !important;
  }
  
  .glass-effect {
    @apply backdrop-blur-md border transition-all duration-300;
    background-color: rgba(255, 255, 255, 0.8);
    border-color: rgba(0, 0, 0, 0.1);
    box-shadow: 0 4px 6px -1px rgba(0, 0, 0, 0.1), 0 2px 4px -1px rgba(0, 0, 0, 0.06);
  }
  
  .dark .glass-effect {
    background-color: rgba(15, 23, 42, 0.8);
    border-color: rgba(71, 85, 105, 0.5);
    box-shadow: 0 4px 6px -1px rgba(0, 0, 0, 0.3), 0 2px 4px -1px rgba(0, 0, 0, 0.2);
  }
  
  .neon-border {
    @apply border border-primary-500/50;
    box-shadow: 0 0 10px rgba(14, 165, 233, 0.3);
  }
  
  /* Enhanced CSS-based matrix background pattern */
  .matrix-bg {
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    background: 
      radial-gradient(circle at 20% 50%, rgba(22, 146, 79, 0.15) 0%, transparent 50%),
      radial-gradient(circle at 80% 20%, rgba(14, 165, 233, 0.15) 0%, transparent 50%),
      radial-gradient(circle at 40% 80%, rgba(22, 146, 79, 0.08) 0%, transparent 50%),
      radial-gradient(circle at 60% 30%, rgba(14, 165, 233, 0.08) 0%, transparent 50%);
    opacity: 0.8;
  }
  
  .matrix-bg::before {
    content: '';
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    background-image: 
      linear-gradient(90deg, transparent 96%, rgba(22, 146, 79, 0.15) 100%),
      linear-gradient(0deg, transparent 96%, rgba(14, 165, 233, 0.15) 100%);
    background-size: 40px 40px;
    animation: matrixGrid 15s linear infinite;
  }
  
  /* Floating Orbs */
  .floating-orb {
    position: absolute;
    border-radius: 50%;
    filter: blur(1px);
    opacity: 0.4;
    animation-timing-function: ease-in-out;
    animation-iteration-count: infinite;
    animation-direction: alternate;
  }
  
  .orb-1 {
    width: 120px;
    height: 120px;
    background: radial-gradient(circle, rgba(14, 165, 233, 0.3) 0%, transparent 70%);
    top: 10%;
    left: 10%;
    animation: floatOrb1 8s infinite;
    filter: blur(4px) brightness(1.1);
  }
  
  .orb-2 {
    width: 80px;
    height: 80px;
    background: radial-gradient(circle, rgba(34, 197, 94, 0.4) 0%, transparent 70%);
    top: 20%;
    right: 15%;
    animation: floatOrb2 12s infinite;
    filter: blur(2px) brightness(1.2);
  }
  
  .orb-3 {
    width: 100px;
    height: 100px;
    background: radial-gradient(circle, rgba(14, 165, 233, 0.2) 0%, transparent 70%);
    bottom: 30%;
    left: 20%;
    animation: floatOrb3 10s infinite;
    filter: blur(5px) brightness(1.0);
  }
  
  .orb-4 {
    width: 60px;
    height: 60px;
    background: radial-gradient(circle, rgba(34, 197, 94, 0.3) 0%, transparent 70%);
    bottom: 15%;
    right: 25%;
    animation: floatOrb4 14s infinite;
    filter: blur(2px) brightness(1.3);
  }
  
  .orb-5 {
    width: 90px;
    height: 90px;
    background: radial-gradient(circle, rgba(14, 165, 233, 0.25) 0%, transparent 70%);
    top: 60%;
    left: 50%;
    animation: floatOrb5 9s infinite;
    filter: blur(3px) brightness(1.1);
  }
  
  /* Animated Particles */
  .particles-container {
    position: absolute;
    width: 100%;
    height: 100%;
    overflow: hidden;
  }
  
  .particle {
    position: absolute;
    width: 4px;
    height: 4px;
    background: rgba(14, 165, 233, 0.6);
    border-radius: 50%;
    animation-timing-function: linear;
    animation-iteration-count: infinite;
  }
  
  .particle-1 {
    top: 20%;
    left: 10%;
    animation: particleFloat1 15s infinite;
  }
  
  .particle-2 {
    top: 40%;
    left: 80%;
    background: rgba(34, 197, 94, 0.6);
    animation: particleFloat2 18s infinite;
  }
  
  .particle-3 {
    top: 70%;
    left: 30%;
    animation: particleFloat3 12s infinite;
  }
  
  .particle-4 {
    top: 15%;
    left: 60%;
    background: rgba(34, 197, 94, 0.6);
    animation: particleFloat4 20s infinite;
  }
  
  .particle-5 {
    top: 80%;
    left: 70%;
    animation: particleFloat5 16s infinite;
  }
  
  .particle-6 {
    top: 50%;
    left: 20%;
    background: rgba(34, 197, 94, 0.6);
    animation: particleFloat6 14s infinite;
  }
  
  .particle-7 {
    top: 35%;
    left: 75%;
    background: rgba(14, 165, 233, 0.7);
    animation: particleFloat7 13s infinite;
  }
  
  .particle-8 {
    top: 85%;
    left: 45%;
    background: rgba(34, 197, 94, 0.5);
    animation: particleFloat8 17s infinite;
  }
  
  .particle-9 {
    top: 25%;
    left: 35%;
    background: rgba(14, 165, 233, 0.6);
    animation: particleFloat9 11s infinite;
  }
  
  .particle-10 {
    top: 75%;
    left: 85%;
    background: rgba(34, 197, 94, 0.7);
    animation: particleFloat10 15s infinite;
  }
  
  /* Gradient Waves */
  .gradient-wave {
    position: absolute;
    width: 200%;
    height: 200%;
    opacity: 0.1;
    border-radius: 50%;
    filter: blur(2px);
  }
  
  .wave-1 {
    background: conic-gradient(from 0deg, rgba(14, 165, 233, 0.3), rgba(34, 197, 94, 0.3), rgba(14, 165, 233, 0.3));
    top: -50%;
    left: -50%;
    animation: rotateWave1 25s linear infinite;
  }
  
  .wave-2 {
    background: conic-gradient(from 180deg, rgba(34, 197, 94, 0.2), rgba(14, 165, 233, 0.2), rgba(34, 197, 94, 0.2));
    bottom: -50%;
    right: -50%;
    animation: rotateWave2 30s linear infinite reverse;
  }
  
  @keyframes matrixGrid {
    0% { transform: translate(0, 0); }
    100% { transform: translate(40px, 40px); }
  }
  
  /* Enhanced Floating Orb Animations - More pronounced movement */
  @keyframes floatOrb1 {
    0%, 100% { transform: translate(0, 0) scale(1); }
    25% { transform: translate(50px, -40px) scale(1.3); }
    50% { transform: translate(-40px, -60px) scale(0.8); }
    75% { transform: translate(60px, -20px) scale(1.15); }
  }
  
  @keyframes floatOrb2 {
    0%, 100% { transform: translate(0, 0) scale(1); }
    33% { transform: translate(-45px, 50px) scale(1.4); }
    66% { transform: translate(55px, 25px) scale(0.7); }
  }
  
  @keyframes floatOrb3 {
    0%, 100% { transform: translate(0, 0) scale(1); }
    20% { transform: translate(40px, -50px) scale(1.2); }
    40% { transform: translate(-50px, -40px) scale(0.8); }
    60% { transform: translate(45px, 45px) scale(1.25); }
    80% { transform: translate(-25px, 20px) scale(0.9); }
  }
  
  @keyframes floatOrb4 {
    0%, 100% { transform: translate(0, 0) scale(1); }
    50% { transform: translate(-60px, -55px) scale(1.5); }
  }
  
  @keyframes floatOrb5 {
    0%, 100% { transform: translate(0, 0) scale(1); }
    25% { transform: translate(-55px, 40px) scale(0.7); }
    50% { transform: translate(40px, -45px) scale(1.3); }
    75% { transform: translate(50px, 50px) scale(0.85); }
  }
  
  /* Enhanced Particle Animations - More dramatic movement */
  @keyframes particleFloat1 {
    0%, 100% { transform: translate(0, 0); opacity: 0.8; }
    25% { transform: translate(150px, -80px); opacity: 1; }
    50% { transform: translate(300px, 50px); opacity: 0.3; }
    75% { transform: translate(200px, -120px); opacity: 0.9; }
  }
  
  @keyframes particleFloat2 {
    0%, 100% { transform: translate(0, 0); opacity: 0.7; }
    33% { transform: translate(-180px, 90px); opacity: 1; }
    66% { transform: translate(-120px, -60px); opacity: 0.2; }
  }
  
  @keyframes particleFloat3 {
    0%, 100% { transform: translate(0, 0); opacity: 0.9; }
    20% { transform: translate(120px, -150px); opacity: 0.3; }
    40% { transform: translate(240px, -90px); opacity: 1; }
    60% { transform: translate(180px, 40px); opacity: 0.5; }
    80% { transform: translate(60px, -50px); opacity: 0.8; }
  }
  
  @keyframes particleFloat4 {
    0%, 100% { transform: translate(0, 0); opacity: 0.8; }
    50% { transform: translate(-300px, 200px); opacity: 1; }
  }
  
  @keyframes particleFloat5 {
    0%, 100% { transform: translate(0, 0); opacity: 0.6; }
    25% { transform: translate(-90px, -180px); opacity: 0.9; }
    50% { transform: translate(-210px, -120px); opacity: 0.2; }
    75% { transform: translate(-150px, -240px); opacity: 1; }
  }
  
  @keyframes particleFloat6 {
    0%, 100% { transform: translate(0, 0); opacity: 0.8; }
    33% { transform: translate(270px, -135px); opacity: 0.3; }
    66% { transform: translate(180px, 60px); opacity: 1; }
  }
  
  /* New particle animations for additional particles */
  @keyframes particleFloat7 {
    0%, 100% { transform: translate(0, 0); opacity: 0.7; }
    25% { transform: translate(-100px, 120px); opacity: 1; }
    50% { transform: translate(80px, -100px); opacity: 0.4; }
    75% { transform: translate(-60px, 80px); opacity: 0.8; }
  }
  
  @keyframes particleFloat8 {
    0%, 100% { transform: translate(0, 0); opacity: 0.6; }
    50% { transform: translate(200px, -180px); opacity: 1; }
  }
  
  @keyframes particleFloat9 {
    0%, 100% { transform: translate(0, 0); opacity: 0.8; }
    33% { transform: translate(-150px, -100px); opacity: 0.3; }
    66% { transform: translate(100px, -150px); opacity: 1; }
  }
  
  @keyframes particleFloat10 {
    0%, 100% { transform: translate(0, 0); opacity: 0.7; }
    25% { transform: translate(120px, 100px); opacity: 0.4; }
    50% { transform: translate(-80px, 150px); opacity: 1; }
    75% { transform: translate(160px, -80px); opacity: 0.6; }
  }
  
  /* Enhanced animation classes for more pronounced effects */
  .enhanced-orb {
    filter: blur(3px) brightness(1.2);
    box-shadow: 
      0 0 30px rgba(14, 165, 233, 0.4),
      0 0 60px rgba(34, 197, 94, 0.3);
  }
  
  .enhanced-particle {
    filter: brightness(1.3);
    box-shadow: 0 0 8px currentColor;
  }
  
  .enhanced-wave {
    opacity: 0.15;
    filter: blur(1px) brightness(1.1);
  }
  
  @keyframes rotateWave1 {
    0% { transform: rotate(0deg); }
    100% { transform: rotate(360deg); }
  }
  
  @keyframes rotateWave2 {
    0% { transform: rotate(0deg); }
    100% { transform: rotate(360deg); }
  }
  
  /* Card styles for better definition */
  .card-light {
    @apply bg-white border border-gray-200 shadow-md;
  }
  
  .dark .card-light {
    @apply bg-gray-800 border-gray-700 shadow-lg;
  }
  
  /* Enhanced hover effects */
  .card-hover {
    @apply transition-all duration-300 hover:shadow-lg hover:-translate-y-1;
  }
  
  .dark .card-hover:hover {
    @apply shadow-2xl;
  }
  
  /* Section backgrounds */
  .section-light {
    background-color: var(--bg-secondary);
  }
  
  .section-dark {
    background-color: var(--bg-primary);
  }
  
  /* Dark mode section overrides */
  .dark .section-light {
    background-color: rgb(30, 41, 59) !important;
  }
  
  .dark .section-dark {
    background-color: rgb(15, 23, 42) !important;
  }
  
  /* Text colors */
  .text-primary {
    color: var(--text-primary);
  }
  
  .text-secondary {
    color: var(--text-secondary);
  }
  
  .text-muted {
    color: var(--text-muted);
  }
}

